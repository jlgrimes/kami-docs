{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docs:generate": "node ./scripts/generate-component-docs.mjs",
    "predev": "npm run docs:generate && fumadocs-mdx",
    "dev": "next dev",
    "prebuild": "npm run docs:generate && fumadocs-mdx",
    "build": "next build",
    "start": "next start",
    "types:check": "npm run docs:generate && fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "npm run docs:generate && fumadocs-mdx",
    "lint": "eslint"
  },
  "dependencies": {
    "@kamiui/kami": "^0.1.0",
    "fumadocs-core": "16.6.5",
    "fumadocs-mdx": "14.2.8",
    "fumadocs-ui": "16.6.5",
    "lucide-react": "^0.570.0",
    "next": "16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "eslint-config-next": "16.1.6",
    "eslint": "^10.0.0"
  }
}
